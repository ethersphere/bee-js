{
  "name": "@ethersphere/bee-js",
  "version": "0.1.0",
  "description": "Storage library for Bee",
  "keywords": [
    "Bee",
    "Swarm",
    "Decentralised Storage"
  ],
  "homepage": "https://github.com/ethersphere/bee-js",
  "bugs": {
    "url": "https://github.com/ethersphere/bee-js/issues/"
  },
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethersphere/bee-js.git"
  },
  "scripts": {
    "postinstall": "patch-package",
    "compile:node": "webpack --progress --mode development --env target=node",
    "compile:browser": "webpack --progress --mode development --env target=web",
    "docs": "typedoc --mode modules --excludeNotExported --readme none --out docs src",
    "test": "jest --config=jest.config.ts",
    "test:browser": "jest --config=jest.config.ts --selectProjects=dom",
    "test:node": "jest --config=jest.config.ts --selectProjects=node",
    "lint": "eslint --fix \"src/**/*.ts\" \"test/**/*.ts\" && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:check": "eslint \"src/**/*.ts\" \"test/**/*.ts\" && prettier --check \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "axios": "^0.21.0",
    "content-disposition": "^0.5.3",
    "readable-stream": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/glob": "^7.1.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/content-disposition": "^0.5.3",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.15",
    "@types/karma": "^5.0.1",
    "@types/node": "^14.14.6",
    "@types/readable-stream": "^2.3.9",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack": "^4.41.25",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@types/yargs-parser": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "debug": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.5.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "yargs-parser": "^20.2.4"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
